{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "ESNext",
    "target": "es2016",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "emitDeclarationOnly": true,
    "sourceMap": true,
    "declarationDir": "types",
    "allowSyntheticDefaultImports": true,
    "jsx": "react"
  }
}
